{
    "entry": ["./src/pages/**/*.{jsx,tsx,js,ts}"],
    "extensions": [".ts", ".js", ".tsx", ".jsx"],
    "ignorePatterns": ["**/node_modules/**", "out/**", ".next/"],
    "ignoreUnresolved": [],
    "ignoreUnimported": ["**/*.d.ts"],
    "ignoreUnused": [],
    "rootDir": ".",
    "typescript": true,
    "aliases": {
        "components/*": ["src/components/*"],
        "fullData/*": ["fullData/*"],
        "utils/*": ["src/utils/*"],
        "hooks/*": ["src/hooks/*"],
        "styles/*": ["src/styles/*"],
        "generated/*": ["src/generated/*"]
    }
}
